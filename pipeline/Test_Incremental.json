{
	"name": "Test_Incremental",
	"properties": {
		"activities": [
			{
				"name": "get incremental max date",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SnowflakeSource",
						"query": "select max(loaddate) as md \nfrom dev.stage.inc_dallasbi\n",
						"exportSettings": {
							"type": "SnowflakeExportCopyCommand"
						}
					},
					"dataset": {
						"referenceName": "MaxDateInc",
						"type": "DatasetReference"
					}
				}
			},
			{
