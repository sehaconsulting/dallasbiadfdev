				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "Snowflake_KeyVault",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "set md =  (select max(loaddate) as md from dev.stage.inc_dallasbi);\n--select $md\ninsert into stage.inc_dallasbi  \nselect * from stage.orig_dallasbi\nwhere loaddate > $md"
